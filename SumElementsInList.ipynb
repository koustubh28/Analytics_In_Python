    "# define a function\n",
    "def sum_list(numbers):\n",
    "  total = 0\n",
    "\n",
    "  # Loop to iterate to pick each number from a list\n",
    "  for num in numbers:\n",
    "    total += num\n",
    "\n",
    "  return total\n",
    "\n",
    "print(sum_list([1, 2, 3, 4, 5]))"
   ]
  }
